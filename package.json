    "rollup": "^0.21.2",
    "rollup-plugin-babel": "^2.2.0"
  },
  "scripts": {
    "build": "rollup -c -f es6 -o dist/gurgle.es6.js && rollup -c -f umd -o dist/gurgle.umd.js -n gurgle",
