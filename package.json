{
  "name": "gurgle",
  "version": "0.1.4",
  "author": "Rich Harris",
  "repository": "https://github.com/Rich-Harris/gurgle",
  "license": "MIT",
  "main": "dist/gurgle.umd.js",
  "jsnext:main": "dist/gurgle.es6.js",
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.0.0",
    "babel-register": "^6.3.13",
    "eslint": "^1.10.3",
    "mocha": "^2.3.4",
    "rollup": "^0.21.2",
    "rollup-plugin-babel": "^2.2.0"
  },
  "scripts": {
    "build": "npm run build:es6 && npm run build:umd",
    "build:es6": "rollup -c -f es6 -o dist/gurgle.es6.js",
    "build:umd": "rollup -c -f umd -o dist/gurgle.umd.js -n gurgle",
    "prepublish": "npm run test && npm run build:umd",
    "test": "mocha test/test.js --compilers js:babel-register",
    "pretest": "npm run build:umd"
  }
}
